<Window x:Class="LifeStory.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LifeStory"
        mc:Ignorable="d"
        Title="LifeStory" Height="450" Width="700" Closing="Window_Closing" ResizeMode="NoResize">
    <Grid>
        <Image Source="/Background.jpg" Stretch="Fill" Opacity="1" Panel.ZIndex="0"/>
        <Frame x:Name="mainWindowFrame" NavigationUIVisibility="Hidden" Panel.ZIndex="0"/>
    </Grid>
</Window>
